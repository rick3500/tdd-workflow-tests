{
  "name": "tdd-workflow-tests",
  "version": "1.0.0",
  "description": "Auto-generated TDD tests from n8n workflow",
  "scripts": {
    "test": "jest tests/generated/**/*.test.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
    "jest": "^29.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "tests/generated/**/*.js"
    ]
  }
}